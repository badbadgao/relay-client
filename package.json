    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "aphrodite": "^2.4.0",
    "graphql": "^15.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^9.1.0",
    "react-scripts": "5.0.1"
  },
